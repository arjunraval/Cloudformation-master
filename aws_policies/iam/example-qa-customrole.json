{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": ["s3:ListBucket"],
      "Resource": ["arn:aws:s3:::example-app-s3-qa1"]
    },
    {
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:PutObjectAcl",
        "s3:PutObjectVersionAcl"
        "s3:GetObject",
        "s3:GetObjectAcl"
        "s3:DeleteObject",
        "s3:DeleteObjectAcl"
 
      ],
      "Resource": ["arn:aws:s3:::example-app-s3-qa1/*"]
    }
  ]
}
